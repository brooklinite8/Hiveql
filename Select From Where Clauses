>Select * from south_pole where celcius < -65;

>Select count (*) 

>EXPLAIN SELECT AVG(CELSIUS) FROM ANTARCTICA.TEMPERATURE;

> select * from south_pole limit 3;

> SELECT DISTINCT MONTH FROM SOUTH_POLE;

> SELECT * FROM SOUTH_POLE WHERE CELSIUS <-65 AND YEAR = 2000 

> SELECT * FROM SOUTH_POLE WHERE CELSIUS <-65 OR YEAR = 2000 

